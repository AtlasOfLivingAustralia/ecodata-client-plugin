{
  "modelName": "Select2 example",
  "dataModel": [
    {
      "dataType": "text",
      "name": "item1",
      "constraints": [
        "value 1",
        "value 2",
        "value 3"
      ]
    },
    {
      "dataType": "text",
      "name": "item2",
      "constraints": [
        "value 1",
        "value 2",
        "value 3"
      ]
    },
    {
      "dataType": "list",
      "name": "nested",
      "columns": [
        {
          "dataType":"number",
          "name": "item3"
        },
        {
          "dataType": "number",
          "name": "item4",
          "computed": {
            "expression": "item3*item2"
          }
        }
      ]
    }
  ],
  "viewModel": [
    {
      "type": "row",
      "items": [
        {
          "type": "col",
          "items": [
            {
              "preLabel": "Item 1 - select2 with defaults",
              "source": "item1",
              "type": "select2"
            },
            {
              "preLabel": "Item 2 - select2 with display options",
              "source": "item2",
              "type": "select2",
              "displayOptions": {
                "tags": true,
                "placeholder": "Custom placeholder"
              }
            }
          ]
        }
      ]
    }
  ],
  "title": "Select2 example"
}